    </webLinks>
    <webLinks>
        <fullName>Send_Task</fullName>
        <availability>online</availability>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Assign Mass Task</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>true</requireRowSelection>
        <url>// Include and initialize the AJAX toolkit library 
{!REQUIRESCRIPT(&quot;/soap/ajax/19.0/connection.js&quot;)}; 
{!REQUIRESCRIPT(&quot;/soap/ajax/19.0/apex.js&quot;)}; 
var idList = {!GETRECORDIDS($ObjectType.Lead)}; 
var retURL = window.location.toString(); 
var proceed = true;
if(idList.length &lt;= 0){
    alert(&apos;Please select record(s).&apos;);
    proceed = false;
}   
if ((navigator.userAgent.indexOf(&apos;MSIE&apos;)) &gt;= 0) {
    if(idList.length &gt; 100){
        alert(&apos;Only maximum of 100 records can be selected.&apos;);
        proceed = false;
    }   
}
if(proceed){
    window.location=&quot;/apex/ABSI_Mass_Task_Action?objIds=&quot; + idList + &quot;&amp;retURL=&quot; + retURL;
}</url>
    </webLinks>
